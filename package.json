{
  "name": "vlt-i18n",
  "displayName": "VLT i18n",
  "repository": "https://github.com/Edon-100/react-i18n",
  "description": "i18n replace",
  "license": "PRIVATE",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:transformToI18n"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "transformToI18n",
        "title": "vlt-i18n_transformToI18n"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "nodemon ./out/test/runTest.js",
    "make": "vsce package -o vlt-i18n.vsix",
    "mount": "yarn make;code --install-extension vlt-i18n.vsix;rm -rf vlt-i18n.vsix;",
    "mount:latest": "git pull; yarn mount;"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/uuid": "^8.3.4",
    "@types/vscode": "^1.68.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.18.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.19",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "vsce": "2.9.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "crypto": "^1.0.1",
    "lodash": "^4.17.21",
    "uuid": "^8.3.2"
  }
}
